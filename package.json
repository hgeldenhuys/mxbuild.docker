    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "ascii-art": "^1.5.1",
    "chalk": "^2.4.2",
    "figlet": "^1.2.4",
    "yargs": "^14.0.0"
  }
}
